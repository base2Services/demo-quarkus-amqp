tasks:
    - name: run backend
      command: |
          sdk install java
          sdk install quarkus
          cd reactive-messaging
          export GITPOD_WORKSPACE_BASE=`echo ${GITPOD_WORKSPACE_URL} | sed -E 's/^\s*.*:\/\///g'`
          echo "QUARKUS_DEV_UI_HOSTS=\"8080-${GITPOD_WORKSPACE_BASE}\"" > .env
          ./mvnw compile quarkus:dev